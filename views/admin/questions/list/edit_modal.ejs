<div class="modal modal-large" id="question_list_edit_modal">
    <div class="modal-header">
        <h3>Question List</h3>
        <button type="button" class="close">
            <svg width="34" height="34" viewBox="0 0 34 34" fill="none" xmlns="http://www.w3.org/2000/svg">
                <g clip-path="url(#clip0_7_7)">
                    <path d="M25.5 8.5L8.5 25.5" stroke="#DADADA" stroke-width="2.125" stroke-linecap="round"
                        stroke-linejoin="round" />
                    <path d="M9 9L26 26" stroke="#DADADA" stroke-width="2.125" stroke-linecap="round"
                        stroke-linejoin="round" />
                </g>
                <defs>
                    <clipPath id="clip0_7_7">
                        <rect width="34" height="34" fill="white" />
                    </clipPath>
                </defs>
            </svg>
        </button>
    </div>

    <form id="question_list_edit_form" class="modal-body" method="post" enctype="multipart/form-data">
        <div class="container questions_container question-card">
        </div>
        <div class="modal-footer">
            <div class="options">
                <button class="cancel" type="button">Cancel</button>
                <button class="submit form-submit-button" id="level_edit_btn" type="submit">Submit</button>
            </div>
        </div>
    </form>
</div>
